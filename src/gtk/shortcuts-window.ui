              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="accelerator">&lt;ctrl&gt;]</property>
                <property name="title" translatable="yes">Indent</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="accelerator">&lt;ctrl&gt;[</property>
                <property name="title" translatable="yes">Dedent</property>
              </object>
            </child>
          </object>
        </child>

        <child>
